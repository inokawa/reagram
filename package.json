{
  "name": "reagram",
  "version": "0.0.7",
  "description": "React component to render customizable graph/diagram.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "tsc": "tsc -p . --noEmit",
    "tsc:d": "tsc -p . --outDir lib -d --emitDeclarationOnly",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "rimraf lib && npm run build && npm run tsc:d"
  },
  "dependencies": {
    "@dagrejs/dagre": "0.8.0",
    "dotparser-normalized": "0.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@rollup/plugin-typescript": "6.1.0",
    "@storybook/addon-actions": "6.1.14",
    "@storybook/addon-essentials": "6.1.14",
    "@storybook/addon-links": "6.1.14",
    "@storybook/react": "6.1.14",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.2.2",
    "jest": "^26.6.3",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "rimraf": "3.0.2",
    "rollup": "2.33.3",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/reagram.git"
  },
  "keywords": [
    "react",
    "graph",
    "diagram",
    "dagre",
    "graphviz",
    "dot",
    "svg"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inokawa/reagram/issues"
  },
  "homepage": "https://github.com/inokawa/reagram#readme"
}
